{
  "workbench.colorTheme": "Ruby Red",
  "workbench.colorCustomizations": {
    "activityBar.background": "#1a0000",
    "activityBar.foreground": "#ff3333",
    "editor.background": "#1a0000",
    "editor.foreground": "#f0f0f0",
    "editor.lineHighlightBackground": "#2a0000",
    "editorGroupHeader.tabsBackground": "#150000",
    "sideBar.background": "#150000",
    "statusBar.background": "#150000",
    "tab.activeBackground": "#1a0000",
    "tab.inactiveBackground": "#150000",
    "titleBar.activeBackground": "#150000",
    "terminal.background": "#150000",
    "terminal.foreground": "#ff3333",
    "input.background": "#150000",
    "chat.editor.background": "#150000",
    "editorWidget.background": "#150000"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "foreground": "#664444"
        }
      },
      {
        "scope": [
          "string",
          "constant.other.symbol"
        ],
        "settings": {
          "foreground": "#ff6666"
        }
      },
      {
        "scope": [
          "constant.numeric",
          "constant.language"
        ],
        "settings": {
          "foreground": "#ff9999"
        }
      },
      {
        "scope": [
          "keyword",
          "storage.type",
          "storage.modifier"
        ],
        "settings": {
          "foreground": "#ff3333"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "support.function"
        ],
        "settings": {
          "foreground": "#ffcccc"
        }
      },
      {
        "scope": [
          "variable",
          "support.variable"
        ],
        "settings": {
          "foreground": "#f0f0f0"
        }
      }
    ]
  }
}
